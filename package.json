{
  "author": "",
  "browserslist": {
    "extends": "@wordpress/browserslist-config"
  },
  "devDependencies": {
    "@wordpress/babel-preset-default": "^4.3.0",
    "@wordpress/browserslist-config": "^2.5.0",
    "@wordpress/eslint-plugin": "^2.3.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.12.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier-eslint": "^9.0.0",
    "sass-loader": "^13.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-wordpress": "^14.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-fix-style-only-entries": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "ISC",
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.php": [
      "./vendor/bin/phpcs --extensions=php --warning-severity=8 -s"
    ],
    "*.scss": [
      "stylelint"
    ]
  },
  "name": "@johnwatkins0/jw-plugin-starter",
  "scripts": {
    "build": "webpack -p",
    "lint": "eslint src",
    "stylelint": "stylelint src",
    "watch": "webpack -wd"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-wordpress/scss"
    ]
  },
  "version": "0.0.1"
}
